{
  "name": "api",
  "version": "1.0.0",
  "description": "api manager system",
  "license": "MIT",
  "private": true,
  "author": "yanhao",
  "egg": {
    "typescript": true
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "vue-cli-service serve",
    "dev:server": "egg-bin dev -r egg-ts-helper/register",
    "dev:docs": "vuepress dev docs",
    "build": "npm-run-all --parallel build:*",
    "build:client": "vue-cli-service build",
    "build:docs": "vuepress build docs",
    "start": "egg-scripts start --daemon --title=egg-server-api",
    "stop": "egg-scripts stop --title=egg-server-api",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test:client": "vue-cli-service test:unit",
    "test:server": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "npm-run-all --parallel lint:*",
    "lint:client": "vue-cli-service lint client",
    "lint:server": "tslint --project . -c tslint.json",
    "commit": "git add . && git-cz",
    "clean": "ets clean"
  },
  "dependencies": {
    "ant-design-vue": "^1.1.8",
    "axios": "^0.18.0",
    "dataloader": "^1.4.0",
    "egg": "^2.6.1",
    "egg-cors": "^2.1.1",
    "egg-graphql": "^2.3.0",
    "egg-jwt": "^3.1.4",
    "egg-mongoose": "^3.1.1",
    "egg-scripts": "^2.6.0",
    "vue": "^2.5.17",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-router": "^3.0.1",
    "vue-types": "^1.3.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "egg-bin": "^4.6.2",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.11.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "graphql-tag": "^2.9.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "typescript": "^3.0.0",
    "vue-cli-plugin-apollo": "^0.18.0",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^1.0.0-alpha.23"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/client/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/test/client/unit/**/*.spec.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "gitHooks": {
    "pre-commit": ""
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint client",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint client",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
