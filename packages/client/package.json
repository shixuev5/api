    "timeago.js": "^4.0.0-beta.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-socket.io-extended": "^3.1.0",
    "vue-types": "^1.3.3",
    "vuex": "^3.0.1",
